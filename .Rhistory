# a function that calculate percentage of each software tool
summary.data <- function(origin) {
origin.sym <- rlang::sym(origin)
group_by(data, !!origin.sym) %>%
summarize(
n = n()
) %>%
mutate (percent = paste0(round(n / sum(n) * 100, digits = 2), "%"))
}
